{
  "include": [
    "src/**/*.ts"
  ],
  "compilerOptions": {
    "target": "ES2023",
    "module": "ESNext",
    "allowJs": false,
    "outDir": "dist",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}